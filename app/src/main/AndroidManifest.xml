<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Inf151872_2"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Base.Theme.Inf151872_2"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogActivity"
            android:label="Log Activity"/>
        <activity android:name=".UploaderActivity"
            android:label="Uploader Activity"/>
        <activity android:name=".LoaderActivity"
            android:label="Loader Activity"/>
        <activity android:name=".GamesActivity"
            android:label="Games Activity"/>
        <activity android:name=".ItemActivity"
            android:label="Item Activity"/>
        <activity
            android:name=".ImagesActivity"
            android:label="Images Activity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET"
        tools:ignore="ManifestOrder" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="32"
        tools:ignore="ScopedStorage" />

</manifest>